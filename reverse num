import java.util.Scanner;
class Demo{
    public static void main(String args[])
    {
        Scanner c=new Scanner(System.in);
        int r,num,rev=0,digit;
        System.out.println("enter the num:");
        num=c.nextInt();
        while(num!=0)
        {
            digit=num%10;
            rev=rev*10+digit;
            num/=10;
           
        }
        System.out.println(rev);
    }
}
