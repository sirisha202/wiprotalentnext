import java.util.Scanner;
class Demo{
    public static void main(String args[])
    {
        Scanner c=new Scanner(System.in);
        int r,num,sum=0;
        System.out.println("enter the num:");
        num=c.nextInt();
        while(num!=0)
        {
            r=num%10;
            sum+=r;
            num/=10;
            
        }
        System.out.println(sum);
    }
}
