import java.util.*;
class Numcheck
{
	public static void main(String args[])
	{
		int num;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a num");
		num=sc.nextInt();
		if(num==0)
			{
				System.out.println("zero");
			}
		else if(num>0)
			{
				System.out.println("pos");
			}
		else if(num<0)
			{
				System.out.println("neg");
			}
	}
}
